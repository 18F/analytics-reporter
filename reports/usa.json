{
  "reports": [
    {
      "name": "realtime",
      "frequency": "realtime",
      "realtime": true,
      "query": {
        "metrics": ["activeUsers"]
      },
      "meta": {
        "name": "Active Users Right Now",
        "description": "Number of users currently visiting all sites."
      }
    },
    {
      "name": "today",
      "frequency": "hourly",
      "query": {
        "dimensions": ["date", "hour"],
        "metrics": ["sessions"],
        "start-date": "today",
        "end-date": "today"
      },
      "meta": {
        "name": "Today",
        "description": "Today's visits for all sites."
      }
    },
    {
      "name": "devices",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date" ,"deviceCategory"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "sort": "date"
      },
      "meta": {
        "name": "Devices",
        "description": "90 days of desktop/mobile/tablet visits for all sites."
      }
    },

    {
      "name": "screen-size",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date" ,"screenResolution"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "sort": "date",
        "filters": ["sessions>1000"]
      },
      "meta": {
        "name": "Screen Resolution",
        "description": "90 days of Screen Resolution visits for all sites. (>5000 sessions)"
      }
    },

    {
      "name": "language",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date" ,"language"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "sort": "date",
        "filters": ["sessions>10"]
      },
      "meta": {
        "name": "Browser Language",
        "description": "90 days of visits by browser language for all sites. (>1000 sessions)"
      }
    },


    {
      "name": "device_model",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date" ,"mobileDeviceModel"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "sort": "date",
        "filters": ["sessions>100"]
      },
      "meta": {
        "name": "Device Model",
        "description": "90 days of visits by Device Model for all sites. (>1000 sessions)"
      }
    },

    {
      "name": "os",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date" ,"operatingSystem"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "filters": ["sessions>10"],
        "sort": "date"
      },
      "meta": {
        "name": "Operating Systems",
        "description": "90 days of visits, broken down by operating system and date, for all sites. (>100 sessions)"
      }
    },
    {
      "name": "windows",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date" ,"operatingSystemVersion"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "filters": [
          "operatingSystem==Windows",
          "sessions>10"
        ],
        "sort": "date"
      },
      "meta": {
        "name": "Windows",
        "description": "90 days of visits from Windows users, broken down by operating system version and date, for all sites. (>100 sessions)"
      }
    },
    {
      "name": "browsers",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date" ,"browser"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "sort": "date,sessions",
        "filters": ["sessions>100"]
      },
      "meta": {
        "name": "Browsers",
        "description": "90 days of visits broken down by browser for all sites. (>100 sessions)"
      }
    },
    {
      "name": "ie",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date","browserVersion"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "sort": "date,sessions",
        "filters": [
          "browser==Internet Explorer",
          "sessions>10"
        ]
      },
      "meta": {
        "name": "Internet Explorer",
        "description": "90 days of visits from Internet Explorer users broken down by version for all sites. (>100 sessions)"
      }
    },
    {
      "name": "os-browsers",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date" ,"browser", "operatingSystem"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "sort": "date,sessions",
        "filters": ["sessions>10"]
      },
      "meta": {
        "name": "OS-browser combinations",
        "description": "90 days of visits broken down by browser and OS for all sites. (>100 sessions)"
      }
    },
    {
      "name": "windows-browsers",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date" ,"browser", "operatingSystemVersion"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "sort": "date,sessions",
        "filters": [
          "sessions>10",
          "operatingSystem==Windows"
        ]
      },
      "meta": {
        "name": "Windows-browser combinations",
        "description": "90 days of visits broken down by Windows versions and browser for all sites. (>100 sessions)"
      }
    },
    {
      "name": "windows-ie",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date","browserVersion", "operatingSystemVersion"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "sort": "date,sessions",
        "filters": [
          "sessions>10",
          "browser==Internet Explorer",
          "operatingSystem==Windows"
        ]
      },
      "meta": {
        "name": "IE on Windows",
        "description": "90 days of visits from IE on Windows broken down by IE and Windows versions for all sites. (>100 sessions)"
      }
    },
    {
      "name": "top-pages-realtime",
      "frequency": "realtime",
      "realtime": true,
      "query": {
        "dimensions": ["pagePath", "pageTitle"],
        "metrics": ["activeUsers"],
        "sort": "activeUsers",
        "max-results": "20",
        "filters":[
          "pagePath!~ncbi\\.nlm\\.nih\\.gov/ncbi_.*"
        ]
      },
      "meta": {
        "name": "Top Pages (Live)",
        "description": "The top 20 pages, measured by active onsite users, for all sites."
      }
    },
    {
      "name": "top-domains-7-days",
      "frequency": "daily",
      "query": {
        "dimensions": ["hostname"],
        "metrics": ["sessions"],
        "start-date": "7daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "max-results": "20"
      },
      "meta": {
        "name": "Top Domains (7 Days)",
        "description": "Last week's top 20 domains, measured by visits, for all sites."
      }
    },
    {
      "name": "top-domains-30-days",
      "frequency": "daily",
      "query": {
        "dimensions": ["hostname"],
        "metrics": ["sessions", "pageviews", "users", "pageviewsPerSession", "avgSessionDuration", "exits"],
        "start-date": "30daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "max-results": "20"
      },
      "meta": {
        "name": "Top Domains (30 Days)",
        "description": "Last 30 days' top 20 domains, measured by visits, for all sites."
      }
    },

    {
      "name": "top-landing-pages-30-days",
      "frequency": "daily",
      "query": {
        "dimensions": ["landingPagePath"],
        "metrics": ["sessions", "pageviews", "users", "pageviewsPerSession", "avgSessionDuration", "exits"],
        "start-date": "30daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "max-results": "20"
      },
      "meta": {
        "name": "Top Landing Pages (30 Days)",
        "description": "Last 30 days' Landing Pages, measured by visits, for all sites."
      }
    },

    {
      "name": "top-traffic-sources-30-days",
      "frequency": "daily",
      "query": {
        "dimensions": ["source", "hasSocialSourceReferral"],
        "metrics": ["sessions", "pageviews", "users", "pageviewsPerSession", "avgSessionDuration", "exits"],
        "start-date": "30daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "max-results": "20"
      },
      "meta": {
        "name": "Top Traffic Sources (30 Days)",
        "description": "Last 30 days' Traffic Sources, measured by visits, for all sites."
      }
    },

    {
      "name": "top-exit-pages-30-days",
      "frequency": "daily",
      "query": {
        "dimensions": ["exitPagePath"],
        "metrics": ["sessions", "pageviews", "users", "pageviewsPerSession", "avgSessionDuration", "exits"],
        "start-date": "30daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "max-results": "20"
      },
      "meta": {
        "name": "Top Exit Pages (30 Days)",
        "description": "Last 30 days' Exit page paths, measured by visits, for all sites."
      }
    },
    {
      "name": "second-level-domains",
      "frequency": "daily",
      "cut": ["sessions"],
      "query": {
        "dimensions": ["hostname"],
        "metrics": ["sessions"],
        "filters": [
          "sessions>4",
          "hostname=~^[^\\.]+\\.[^\\.]+$"
        ],
        "start-date": "14daysAgo",
        "end-date": "yesterday",
        "sort": "hostname",
        "max-results": 10000
      },
      "meta": {
        "name": "Participating second-level domains.",
        "description": "Participating second-level domains over the last 2 weeks."
      }
    },
    {
      "name": "sites",
      "frequency": "daily",
      "cut": ["sessions"],
      "query": {
        "dimensions": ["hostname"],
        "metrics": ["sessions"],
        "filters": [
          "sessions>9"
        ],
        "start-date": "14daysAgo",
        "end-date": "yesterday",
        "sort": "hostname",
        "max-results": 10000
      },
      "meta": {
        "name": "Participating hostnames.",
        "description": "Participating hostnames over the last 14 days with at least 10 visits."
      }
    },
    {
      "name": "sites-extended",
      "frequency": "daily",
      "cut": ["sessions"],
      "query": {
        "dimensions": ["hostname"],
        "metrics": ["sessions"],
        "filters": [
          "sessions>1"
        ],
        "start-date": "14daysAgo",
        "end-date": "yesterday",
        "sort": "hostname",
        "max-results": 10000
      },
      "meta": {
        "name": "Participating hostnames.",
        "description": "Participating hostnames over the last 14 days with at least 1 visit."
      }
    },
    {
      "name": "top-downloads-yesterday",
      "frequency": "daily",
      "query": {
        "dimensions": ["pageTitle", "eventLabel", "pagePath"],
        "metrics": ["totalEvents"],
        "filters": [
          "eventCategory=~^(download|downloads|(outbound downloads))$",
          "pagePath!~(usps.com).*\/(?i)(zip|doc).*"
        ],
        "start-date": "yesterday",
        "end-date": "yesterday",
        "sort": "totalEvents",
        "max-results": "100"
      },
      "meta": {
        "name": "Top Downloads Yesterday",
        "description": "Top downloads yesterday"
      }
    },
    {
      "name": "top-downloads-7-days",
      "frequency": "daily",
      "query": {
        "dimensions": ["pageTitle", "eventLabel", "pagePath"],
        "metrics": ["totalEvents"],
        "filters": [
          "eventCategory=~^(download|downloads|(outbound downloads))$",
          "eventLabel!~swf$",
          "pagePath!~(usps.com).*\/(?i)(zip|doc).*"
        ],
        "start-date": "7daysAgo",
        "end-date": "yesterday",
        "sort": "totalEvents",
        "max-results": "100"
      },
      "meta": {
        "name": "Top Downloads (7 Days)",
        "description": "Top downloads in the last 7 days."
      }
    },
    {
      "name": "top-cities-realtime",
      "frequency": "realtime",
      "realtime": true,
      "query": {
        "dimensions": ["city"],
        "metrics": ["activeUsers"],
        "sort": "activeUsers"
      },
      "meta": {
        "name": "Top Cities (Live)",
        "description": "Top cities for active onsite users."
      }
    },
    {
      "name": "top-countries-realtime",
      "frequency": "realtime",
      "realtime": true,
      "query": {
        "dimensions": ["country"],
        "metrics": ["activeUsers"],
        "sort": "activeUsers"
      },
      "meta": {
        "name": "Top Cities",
        "description": "Top countries for active onsite users."
      }
    },
    {
      "name": "all-pages-realtime",
      "frequency": "realtime",
      "realtime": true,
      "threshold": {
        "field": "activeUsers",
        "value": "10"
      },
      "query": {
        "dimensions": ["pagePath", "pageTitle"],
        "metrics": ["activeUsers"],
        "sort": "activeUsers",
        "max-results": "10000"
      },
      "meta": {
        "name": "All Pages (Live)",
        "description": "Pages, measured by active onsite users, for all sites."
      }
    },
    {
      "name": "all-domains-30-days",
      "frequency": "daily",
      "query": {
        "dimensions": ["hostname"],
        "metrics": ["sessions", "pageviews", "users", "pageviewsPerSession", "avgSessionDuration", "exits"],
        "start-date": "30daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "max-results": "10000",
        "filters": ["sessions>=10"]
      },
      "meta": {
        "name": "All Domains (30 Days)",
        "description": "Last 30 days' domains, measured by visits, for all sites."
      }
    }

  ]
}
