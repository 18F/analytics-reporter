    {
      "name": "today",
      "frequency": "realtime",
      "query": {
        "dimensions": [
          {
            "name": "date"
          },
          {
            "name": "hour"
          }
        ],
        "metrics": [
          {
            "name": "sessions"
          }
        ],
        "dateRanges": [
          {
            "startDate": "today",
            "endDate": "today"
          }
        ],
        "dimensionFilter": {
          "andGroup": {
            "expressions": [
              {
                "notExpression": {
                  "filter": {
                    "fieldName": "hour",
                    "stringFilter": {
                      "value": "(other)",
                      "caseSensitive": false
                    }
                  }
                }
              },
              {
                "notExpression": {
                  "filter": {
                    "fieldName": "hour",
                    "stringFilter": {
                      "value": "null",
                      "caseSensitive": false
                    }
                  }
                }
              },
              {
                "notExpression": {
                  "filter": {
                    "fieldName": "hour",
                    "stringFilter": {
                      "value": "",
                      "caseSensitive": false
                    }
                  }
                }
              }
            ]
          }
        },
        "orderBys": [
          {
            "dimension": {
              "dimensionName": "hour"
            },
            "asc": true
          }
        ]
      },
      "meta": {
        "name": "Today",
        "description": "Today's visits for all sites."
      }
