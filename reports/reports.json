    {
      "name": "top-pages-realtime",
      "frequency": "realtime",
      "realtime": true,
      "query": {
        "dimensions": [
          {
            "name": "unifiedScreenName"
          }
        ],
        "metrics": [
          {
            "name": "activeUsers"
          }
        ],
        "orderBys": [
          {
            "metric": {
              "metricName": "activeUsers"
            },
            "desc": true
          }
        ],
        "dimensionFilter": {
          "andGroup": {
            "expressions": [
              {
                "notExpression": {
                  "filter": {
                    "fieldName": "unifiedScreenName",
                    "stringFilter": {
                      "value": "(other)",
                      "caseSensitive": false
                    }
                  }
                }
              },
              {
                "notExpression": {
                  "filter": {
                    "fieldName": "unifiedScreenName",
                    "stringFilter": {
                      "value": "null",
                      "caseSensitive": false
                    }
                  }
                }
              },
              {
                "notExpression": {
                  "filter": {
                    "fieldName": "unifiedScreenName",
                    "stringFilter": {
                      "value": "",
                      "caseSensitive": false
                    }
                  }
                }
              },
              {
                "notExpression": {
                  "filter": {
                    "fieldName": "unifiedScreenName",
                    "stringFilter": {
                      "matchType": "FULL_REGEXP",
                      "value": "\/(.+)$",
                      "caseSensitive": false
                    }
                  }
                }
              }
            ]
          }
        },
        "limit": "20"
      },
      "meta": {
        "name": "Top Pages (Live)",
        "description": "The top 20 pages, measured by active onsite users, for all sites."
      }
    },
