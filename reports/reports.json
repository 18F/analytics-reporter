{
  "reports": [
    {
      "name": "realtime",
      "frequency": "realtime",
      "realtime": true,
      "query": {
        "metrics": ["activeUsers"]
      },
      "meta": {
        "name": "Active Users Right Now",
        "description": "Number of users currently visiting all sites."
      }
    },
    {
      "name": "today",
      "frequency": "realtime",
      "query": {
        "dimensions": ["date", "hour"],
        "metrics": ["sessions"],
        "start-date": "today",
        "end-date": "today"
      },
      "meta": {
        "name": "Today",
        "description": "Today's visits for all sites."
      }
    },
    {
      "name": "last-48-hours",
      "frequency": "realtime",
      "query": {
        "dimensions": ["date", "hour"],
        "metrics": ["sessions"],
        "start-date": "yesterday",
        "end-date": "today"
      },
      "meta": {
        "name": "Today",
        "description": "Today's visits for all sites."
      }
    },
    {
      "name": "users",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "sort": "date"
      },
      "meta": {
        "name": "Visitors",
        "description": "90 days of visits for all sites."
      }
    },
    {
      "name": "devices",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date" ,"deviceCategory"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "sort": "date"
      },
      "meta": {
        "name": "Devices",
        "description": "90 days of desktop/mobile/tablet visits for all sites."
      }
    },
    {
      "name": "os",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date" ,"operatingSystem"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "sort": "date"
      },
      "meta": {
        "names": "Operating Systems",
        "description": "90 days of visits, broken down by operating system and date, for all sites."
      }
    },
    {
      "name": "screen-size",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date" ,"screenResolution"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "sort": "date"
      },
      "meta": {
        "name": "Screen Resolution",
        "description": "90 days of Screen Resolution visits for all sites."
      }
    },
    {
      "name": "language",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date" ,"language"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "sort": "date"
      },
      "meta": {
        "name": "Browser Language",
        "description": "90 days of visits by browser language for all sites."
      }
    },
    {
      "name": "device_model",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date" ,"mobileDeviceModel"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "sort": "date"
      },
      "meta": {
        "name": "Device Model",
        "description": "90 days of visits by Device Model for all sites."
      }
    },
    {
      "name": "windows",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date" ,"operatingSystemVersion"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "filters": ["operatingSystem==Windows"],
        "sort": "date"
      },
      "meta": {
        "names": "Windows",
        "description": "90 days of visits from Windows users, broken down by operating system version and date, for all sites."
      }
    },
    {
      "name": "browsers",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date" ,"browser"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "sort": "date,sessions"
      },
      "meta": {
        "name": "Browsers",
        "description": "90 days of visits broken down by browser for all sites."
      }
    },
    {
      "name": "ie",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date","browserVersion"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "sort": "date,sessions",
        "filters": ["browser==Internet Explorer"]
      },
      "meta": {
        "name": "Internet Explorer",
        "description": "90 days of visits from Internet Explorer users broken down by version for all sites."
      }
    },
    {
      "name": "os-browsers",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date" ,"browser", "operatingSystem"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "sort": "date,sessions"
      },
      "meta": {
        "name": "OS-browser combinations",
        "description": "90 days of visits broken down by browser and OS for all sites."
      }
    },
    {
      "name": "windows-browsers",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date" ,"browser", "operatingSystemVersion"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "sort": "date,sessions",
        "filters": [
          "operatingSystem==Windows"
        ]
      },
      "meta": {
        "name": "Windows-browser combinations",
        "description": "90 days of visits broken down by Windows versions and browser for all sites."
      }
    },
    {
      "name": "windows-ie",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": ["date","browserVersion", "operatingSystemVersion"],
        "metrics": ["sessions"],
        "start-date": "90daysAgo",
        "end-date": "yesterday",
        "sort": "date,sessions",
        "filters": [
          "browser==Internet Explorer",
          "operatingSystem==Windows"
        ]
      },
      "meta": {
        "name": "IE on Windows",
        "description": "90 days of visits from IE on Windows broken down by IE and Windows versions for all sites."
      }
    },
    {
      "name": "top-pages-realtime",
      "frequency": "realtime",
      "realtime": true,
      "query": {
        "dimensions": ["pagePath", "pageTitle"],
        "metrics": ["activeUsers"],
        "sort": "activeUsers",
        "max-results": "20"
      },
      "meta": {
        "name": "Top Pages (Live)",
        "description": "The top 20 pages, measured by active onsite users, for all sites."
      }
    },
    {
      "name": "top-pages-7-days",
      "frequency": "daily",
      "query": {
        "dimensions": ["hostname", "pagePath", "pageTitle"],
        "metrics": ["pageviews"],
        "start-date": "7daysAgo",
        "end-date": "yesterday",
        "sort": "pageviews",
        "max-results": "20"
      },
      "meta": {
        "name": "Top Pages (7 Days)",
        "description": "Last week's top 20 pages, measured by page views, for all sites."
      }
    },
    {
      "name": "top-pages-30-days",
      "frequency": "daily",
      "query": {
        "dimensions": [
          "hostname",
          "pagePath",
          "pageTitle"
        ],
        "metrics": [
          "pageviews",
          "sessions",
          "users",
          "pageviewsPerSession",
          "avgSessionDuration",
          "exits"
        ],
        "start-date": "30daysAgo",
        "end-date": "yesterday",
        "sort": "pageviews",
        "max-results": "20"
      },
      "meta": {
        "name": "Top Pages (30 Days)",
        "description": "Last 30 days' top 20 pages, measured by page views, for all sites."
      }
    },
    {
      "name": "top-domains-7-days",
      "frequency": "daily",
      "query": {
        "dimensions": ["hostname"],
        "metrics": ["sessions"],
        "start-date": "7daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "max-results": "20"
      },
      "meta": {
        "name": "Top Domains (7 Days)",
        "description": "Last week's top 20 domains, measured by visits, for all sites."
      }
    },
    {
      "name": "top-domains-30-days",
      "frequency": "daily",
      "query": {
        "dimensions": ["hostname"],
        "metrics": ["sessions", "users", "pageviews", "pageviewsPerSession", "avgSessionDuration","exits"],
        "start-date": "30daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "max-results": "20"
      },
      "meta": {
        "name": "Top Domains (30 Days)",
        "description": "Last 30 days' top 20 domains, measured by visits, for all sites."
      }
    },

    {
      "name": "top-landing-pages-30-days",
      "frequency": "daily",
      "query": {
        "dimensions": ["landingPagePath"],
        "metrics": ["sessions", "pageviews", "users", "pageviewsPerSession", "avgSessionDuration", "exits"],
        "start-date": "30daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "max-results": "20"
      },
      "meta": {
        "name": "Top Landing Pages (30 Days)",
        "description": "Last 30 days' Landing Pages, measured by visits, for all sites."
      }
    },

    {
      "name": "top-traffic-sources-30-days",
      "frequency": "daily",
      "query": {
        "dimensions": ["source", "hasSocialSourceReferral"],
        "metrics": ["sessions", "pageviews", "users", "pageviewsPerSession", "avgSessionDuration", "exits"],
        "start-date": "30daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "max-results": "20"
      },
      "meta": {
        "name": "Top Traffic Sources (30 Days)",
        "description": "Last 30 days' Traffic sources, measured by visits, for all sites."
      }
    },

    {
      "name": "top-exit-pages-30-days",
      "frequency": "daily",
      "query": {
        "dimensions": ["exitPagePath"],
        "metrics": ["sessions", "pageviews", "users", "pageviewsPerSession", "avgSessionDuration", "exits"],
        "start-date": "30daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "max-results": "20"
      },
      "meta": {
        "name": "Top Exit Pages (30 Days)",
        "description": "Last 30 days' Exit page paths, measured by visits, for all sites."
      }
    }

  ]
}
