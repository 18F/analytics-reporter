{
  "reports": [
    {
      "name": "device",
      "frequency": "daily",
      "query": {
        "dimensions": ["date" ,"deviceCategory"],
        "metrics": ["sessions"],
        "start-date": "3daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "filters": ["sessions>100"]
      },
      "meta": {
        "name": "Devices",
        "description": "Desktop/mobile/tablet visits"
      }
    },
    {
      "name": "language",
      "frequency": "daily",
      "query": {
        "dimensions": ["date" ,"language"],
        "metrics": ["sessions"],
        "start-date": "3daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "filters": [
          "sessions>100"
        ]
      },
      "meta": {
        "name": "Browser Languages",
        "description": "Browser languages of visiting browsers"
      }
    },
    {
      "name": "device-model",
      "frequency": "daily",
      "query": {
        "dimensions": ["date" ,"mobileDeviceModel"],
        "metrics": ["sessions"],
        "start-date": "3daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "filters": ["sessions>100"]
      },
      "meta": {
        "name": "Device Models",
        "description": "Device models of visiting devices"
      }
    },
    {
      "name": "os",
      "frequency": "daily",
      "query": {
        "dimensions": ["date" ,"operatingSystem"],
        "metrics": ["sessions"],
        "start-date": "3daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "filters": ["sessions>100"]
      },
      "meta": {
        "name": "Operating Systems",
        "description": "Operating systems of visiting devices"
      }
    },
    {
      "name": "windows",
      "frequency": "daily",
      "query": {
        "dimensions": ["date" ,"operatingSystemVersion"],
        "metrics": ["sessions"],
        "start-date": "3daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "filters": [
          "operatingSystem==Windows",
          "sessions>100"
        ]
      },
      "meta": {
        "name": "Windows",
        "description": "Operating system of visiting windows devices"
      }
    },
    {
      "name": "browser",
      "frequency": "daily",
      "query": {
        "dimensions": ["date" ,"browser"],
        "metrics": ["sessions"],
        "start-date": "3daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "filters": ["sessions>100"]
      },
      "meta": {
        "name": "Browsers",
        "description": "Browsers of visiting users"
      }
    },
    {
      "name": "ie",
      "frequency": "daily",
      "query": {
        "dimensions": ["date","browserVersion"],
        "metrics": ["sessions"],
        "start-date": "3daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "filters": [
          "browser==Internet Explorer",
          "sessions>100"
        ]
      },
      "meta": {
        "name": "Internet Explorer",
        "description": "Visits from Internet Explorer users broken down by version"
      }
    },
    {
      "name": "os-browser",
      "frequency": "daily",
      "query": {
        "dimensions": ["date" ,"browser", "operatingSystem"],
        "metrics": ["sessions"],
        "start-date": "3daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "filters": ["sessions>100"]
      },
      "meta": {
        "name": "OS-browser combinations",
        "description": "Visits broken down by browser and OS for all sites"
      }
    },
    {
      "name": "windows-browser",
      "frequency": "daily",
      "query": {
        "dimensions": ["date" ,"browser", "operatingSystemVersion"],
        "metrics": ["sessions"],
        "start-date": "3daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "filters": [
          "sessions>100",
          "operatingSystem==Windows"
        ]
      },
      "meta": {
        "name": "Windows-browser combinations",
        "description": "Visits broken down by Windows versions and browser for all sites"
      }
    },
    {
      "name": "windows-ie",
      "frequency": "daily",
      "query": {
        "dimensions": ["date","browserVersion", "operatingSystemVersion"],
        "metrics": ["sessions"],
        "start-date": "3daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "filters": [
          "sessions>100",
          "browser==Internet Explorer",
          "operatingSystem==Windows"
        ]
      },
      "meta": {
        "name": "IE on Windows",
        "description": "Visits from IE on Windows broken down by IE and Windows versions"
      }
    },
    {
      "name": "domain",
      "frequency": "daily",
      "query": {
        "dimensions": ["date", "hostname"],
        "metrics": ["sessions"],
        "start-date": "3daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "filters": ["sessions>100"]
      },
      "meta": {
        "name": "Domains",
        "description": "Number of visitors for a given domain"
      }
    },
    {
      "name": "traffic-source",
      "frequency": "daily",
      "query": {
        "dimensions": ["date", "source", "hasSocialSourceReferral"],
        "metrics": ["sessions"],
        "start-date": "3daysAgo",
        "end-date": "yesterday",
        "sort": "sessions",
        "filters": ["sessions>100"]
      },
      "meta": {
        "name": "Top Traffic Sources",
        "description": "Visitors for a given traffic source"
      }
    },
    {
      "name": "second-level-domain",
      "frequency": "daily",
      "query": {
        "dimensions": ["date", "hostname"],
        "metrics": ["sessions"],
        "sort": "sessions",
        "filters": [
          "sessions>100",
          "hostname=~^[^\\.]+\\.[^\\.]+$"
        ],
        "start-date": "3daysAgo",
        "end-date": "yesterday"
      },
      "meta": {
        "name": "Participating second-level domains.",
        "description": "Visits to participating second-level domains"
      }
    },
    {
      "name": "site",
      "frequency": "daily",
      "query": {
        "dimensions": ["date", "hostname"],
        "metrics": ["sessions"],
        "filters": ["sessions>100"],
        "start-date": "3daysAgo",
        "sort": "sessions",
        "end-date": "yesterday"
      },
      "meta": {
        "name": "Participating hostnames.",
        "description": "Visits to participating hostnames"
      }
    },
    {
      "name": "download",
      "frequency": "daily",
      "query": {
        "dimensions": ["date", "pageTitle", "eventLabel", "pagePath"],
        "metrics": ["totalEvents"],
        "filters": [
          "eventCategory=~^(download|downloads|(outbound downloads))$",
          "pagePath!~(usps.com).*\/(?i)(zip|doc).*",
          "totalEvents>100"
        ],
        "start-date": "3daysAgo",
        "sort": "totalEvents",
        "end-date": "yesterday"
      },
      "meta": {
        "name": "Downloads",
        "description": "Number of download events"
      }
    }
  ]
}
