{
  "name": "analytics-reporter",
  "version": "1.2.2",
  "description": "A lightweight command line tool for reporting and publishing analytics data from a Google Analytics account.",
  "keywords": [
    "analytics",
    "google analytics"
  ],
  "homepage": "https://github.com/18F/analytics-reporter",
  "license": "CC0-1.0",
  "scripts": {
    "migrate": "`npm bin`/knex migrate:latest",
    "pretest": "NODE_ENV=test npm run migrate",
    "start": "node app.js",
    "test": "`npm bin`/mocha test/*.test.js test/**/*.test.js"
  },
  "contributors": [
    {
      "name": "Gabriel Ramirez",
      "email": "gabriel.ramirez@gsa.gov"
    },
    {
      "name": "Eric Mill",
      "email": "eric.mill@gsa.gov"
    },
    {
      "name": "Lauren Ancona",
      "email": "lauren.ancona@phila.gov"
    },
    {
      "name": "Eric Schles",
      "email": "eric.schles@gsa.gov"
    }
  ],
  "files": [
    "bin",
    "src",
    "test",
    "reports",
    "index.js",
    "newrelic.js",
    "package.json",
    "*.md"
  ],
  "engines": {
    "node": "12.x.x"
  },
  "preferGlobal": true,
  "main": "index",
  "bin": {
    "analytics": "./bin/analytics"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/18F/analytics-reporter.git"
  },
  "bugs": {
    "url": "https://github.com/18F/analytics-reporter/issues"
  },
  "dependencies": {
    "ansi-regex": "^3.0.1",
    "aws-sdk": "^2.1404.0",
    "bluebird": "^3.7.2",
    "debug": "^4.1.1",
    "decode-uri-component": "^0.2.2",
    "fast-csv": "^4.3.0",
    "flat": "^5.0.2",
    "googleapis": "^123.0.0",
    "https-proxy-agent": "^5.0.1",
    "json-schema": "^0.4.0",
    "jsprim": "^1.4.2",
    "minimist": "^1.2.6",
    "qs": "^6.5.3",
    "semver": "^7.5.4",
    "stylelint": "^15.10.1",
    "winston-color": "^1.0.0",
    "word-wrap": "^1.2.4",
    "xml2js": "^0.5.0"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "mocha": "^8.4.0",
    "proxyquire": "^2.1.3"
  },
  "optionalDependencies": {
    "knex": "^2.4.0",
    "newrelic": "^10.6.0",
    "pg": "^8.11.1"
  }
}
